{
  "name": "tagmastarna",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "nodemon src/server.js",
    "server:build": "babel --verbose --out-dir dist --only 'src/server.js,src/lib/' src",
    "client:build": "vite build",
    "build": "npm run server:build && npm run client:build",
    "serve": "vite preview",
    "start": "node dist/server.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@vitejs/plugin-react": "^1.0.2",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.1",
    "http-proxy-middleware": "^2.0.1",
    "nodemon": "^2.0.13",
    "postcss": "^8.4.17",
    "prop-types": "^15.7.2",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.5.1",
    "tailwindcss": "^3.1.8",
    "vite": "^2.5.10"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "date-and-time": "^2.4.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-validator": "^6.14.2",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.1.11",
    "mysql2": "^2.3.3",
    "normalize.css": "^8.0.1",
    "process": "^0.11.10",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-icons": "^4.4.0",
    "react-loading": "^2.0.3",
    "react-redux": "^8.0.4",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.4.1",
    "sequelize": "^6.23.2",
    "sweetalert2": "^11.4.38"
  }
}
